{
    "appenders": [
        {
            "type": "console"
        },
        {
            "type": "dateFile",
            "filename": "${opts:base}/memdb-${opts:shardId}.log",
            "pattern": "-yyyyMMdd",
            "layout": {
                "type": "basic"
            },
            "backups": 5,
            "category":"memdb"
        },
        {
            "type": "dateFile",
            "filename": "${opts:base}/memdbclient.log",
            "pattern": "-yyyyMMdd",
            "layout": {
                "type": "basic"
            },
            "backups": 5,
            "category":"memdb-client"
        },
        {
            "type": "dateFile",
            "filename": "${opts:base}/memdbtest.log",
            "pattern": "-yyyyMMdd",
            "layout": {
                "type": "basic"
            },
            "backups": 5,
            "category":"test"
        }
    ],

    "levels" : {
    },

    "replaceConsole": false,
    "lineDebug": false
}
